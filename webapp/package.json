    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "prettier": "^2.0.2",
    "ts-node": "^9.0.0",
    "typescript": "4.0.2"
  }
