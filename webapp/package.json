    "overlayscrollbars-react": "^0.2.2",
    "rc-collapse": "^2.0.0",
    "rc-drawer": "^4.1.0",
    "rc-table": "^7.9.2",
    "react": "^16.13.1",
    "react-content-loader": "^5.1.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.0",
    "react-image": "^4.0.3",
    "react-intl": "^5.8.1",
