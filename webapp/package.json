    "formidable": "^1.2.2",
    "formik": "^2.1.5",
    "fuse.js": "^6.4.1",
    "js-cookie": "^2.2.1",
    "localforage": "^1.9.0",
    "moment": "^2.29.0",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-cookies": "^2.0.3",
    "next-optimized-images": "^2.6.2",
