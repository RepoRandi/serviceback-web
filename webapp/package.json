{
  "name": "sb-web",
  "author": "Christopher Cai",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:local": "yarn local ./node_modules/.bin/next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "local": "DOTENV_CONFIG_PATH=./.env.development.local ts-node -P ./tsconfig.yarn.json -r dotenv/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
